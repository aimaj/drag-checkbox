{

    "manifest_version": 2,
    "name": "drag-checkbox",
    "version": "1.0",
  
    "description": "Checks all checkboxes within a click and dragged area, deselects if all are already checked.",
  
    "permissions": [
    ],

    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["script.js"],
          "match_about_blank": true,
          "all_frames": true
        }
    ]
  
  }